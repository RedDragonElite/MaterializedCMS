                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Cats/Index.php" />
                </rule>						

				<!-- Rewrite Admin Dashboard - Create new Cat Item -->
                <rule name="Rewrite Admin Dashboard - Create Cat Item" stopProcessing="true">
                    <match url="^dashboard/create-cat$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Cats/Create.php" />
                </rule>
				
				<!-- Rewrite Admin Dashboard - Edit Cat Item -->
                <rule name="Rewrite Admin Dashboard - Edit Cat Item" stopProcessing="true">
                    <match url="^dashboard/edit-cat/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Cats/Edit.php?id={R:1}" appendQueryString="true" />
                </rule>					

				<!-- Rewrite Admin Dashboard - Delete Cat Item -->
                <rule name="Rewrite Admin Dashboard - Delete Cat Item" stopProcessing="true">
                    <match url="^dashboard/delete-cat/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Cats/Delete.php?id={R:1}" appendQueryString="true" />
                </rule>	
				
				<!-- Rewrite Admin Dashboard - Create new Sub-Cat Item -->
                <rule name="Rewrite Admin Dashboard - Create Sub-Cat Item" stopProcessing="true">
                    <match url="^dashboard/create-sub-cat$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
