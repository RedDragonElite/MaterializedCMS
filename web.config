                    <match url="^dashboard/delete-staff/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Staff/Delete.php?id={R:1}" appendQueryString="true" />
                </rule>	
				
			<!-- ###################################
				 #                                 #
				 #      Rewrite Nav Menu Rules     #		 
				 #                                 #
				 ################################### -->

				<!-- Rewrite Admin Dashboard - All Menu Items -->
                <rule name="Rewrite Admin Dashboard - All Menu Items" stopProcessing="true">
                    <match url="^dashboard/menu$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Menu/Index.php" />
                </rule>						

				<!-- Rewrite Admin Dashboard - Create new Menu Item -->
                <rule name="Rewrite Admin Dashboard - Create Menu Item" stopProcessing="true">
                    <match url="^dashboard/create-menu$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Menu/Create.php" />
                </rule>
				
				<!-- Rewrite Admin Dashboard - Edit Menu Item -->
                <rule name="Rewrite Admin Dashboard - Edit Menu Item" stopProcessing="true">
                    <match url="^dashboard/edit-menu/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Menu/Edit.php?id={R:1}" appendQueryString="true" />
