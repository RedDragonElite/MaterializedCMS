                    <match url="^dashboard/delete-sub-menu/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Menu/sDelete.php?id={R:1}" appendQueryString="true" />
                </rule>					
								
			<!-- ###################################
				 #                                 #
				 #      Rewrite Category Rules     #		 
				 #                                 #
				 ################################### -->

				<!-- Rewrite Admin Dashboard - All Cat Items -->
