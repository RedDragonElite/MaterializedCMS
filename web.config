                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Menu/sCreate.php" />
                </rule>
				
				<!-- Rewrite Admin Dashboard - Edit Sub-Menu Item -->
