				<!-- Rewrite Admin Dashboard - Delete Menu Item -->
                <rule name="Rewrite Admin Dashboard - Delete Menu Item" stopProcessing="true">
                    <match url="^dashboard/delete-menu/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Menu/Delete.php?id={R:1}" appendQueryString="true" />
                </rule>	
				
				<!-- Rewrite Admin Dashboard - Create new Sub-Menu Item -->
                <rule name="Rewrite Admin Dashboard - Create Sub-Menu Item" stopProcessing="true">
                    <match url="^dashboard/create-sub-menu$" />
