                    <match url="^dashboard/edit-sub-cat/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Cats/sEdit.php?id={R:1}" appendQueryString="true" />
                </rule>					

				<!-- Rewrite Admin Dashboard - Delete Sub-Cat Item -->
