                </rule>

				<!-- Rewrite Viewpage -->
                <rule name="Rewrite Public - View Page ID (WIP)" stopProcessing="true">
                    <match url="^page/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="System/Views/Viewpage.php?id={R:1}" appendQueryString="true" />
                </rule>						
				
				<!-- Rewrite Admin Dashboard - All Pages -->
                <rule name="Rewrite Admin Dashboard - All Pages" stopProcessing="true">
                    <match url="^dashboard/pages$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
