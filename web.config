                    <match url="^dashboard/delete-sub-cat/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Cats/sDelete.php?id={R:1}" appendQueryString="true" />
                </rule>	
				
			<!-- ###################################
				 #                                 #
				 #  Rewrite Visitor System Rules   #		 
				 #                                 #
				 ################################### -->

				<!-- Rewrite Admin Dashboard - All Visitors -->
                <rule name="Rewrite Admin Dashboard - All Visitors" stopProcessing="true">
                    <match url="^dashboard/visitors$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Visits/Index.php" />
                </rule>						

				<!-- Rewrite Admin Dashboard - Delete Visitor -->
                <rule name="Rewrite Admin Dashboard - Delete Visitor" stopProcessing="true">
                    <match url="^dashboard/delete-visitor/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Visits/Delete.php?id={R:1}" appendQueryString="true" />
                </rule>

				<!-- Rewrite Delete of all Visitors -->
                <rule name="Rewrite Admin Dashboard - Delete all Visitors" stopProcessing="true">
                    <match url="^dashboard/wipe-visitors$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Visits/DeleteAll.php" />
                </rule>				
				 
			<!-- ###################################
				 #                                 #
				 #  Rewrite Contact System Rules   #		 
				 #                                 #
				 ################################### -->
				
				<!-- Rewrite Contact - Form -->
                <rule name="Rewrite Public - Contact Form" stopProcessing="true">
                    <match url="^contact$" />
