<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
	
	<!-- ###################################
	     #      .::MaterializedCMS::.      #
	     #   Website Systems Web.Config    #		 
		 # http://www.materializedcms.com/ #
		 ################################### -->
		 
		<defaultDocument enabled="true">
		  <files>
			<clear />
			<add value="index.php" />
			<add value="default.html" />
		  </files>
		</defaultDocument>
		<security>
		  <authentication>
			<basicAuthentication enabled="true" />
		  </authentication>
		</security>
		<httpErrors errorMode="Custom">
		  <remove statusCode="404" subStatusCode="-1" />
		  <error statusCode="404" prefixLanguageFilePath="" path="/System/Error/Error_404.php" responseMode="ExecuteURL" />
		</httpErrors>
		<!-- HIDE ASP.NET -->
		<httpProtocol>
		  <customHeaders>
			<remove name="X-Powered-By" />
		  </customHeaders>
		</httpProtocol>	
		
	<!-- ###################################
		 #                                 #
		 #            Security             #		 
		 #                                 #
		 ################################### -->
		 
        <security>
            <requestFiltering>
                <fileExtensions applyToWebDAV="false">
                     <add fileExtension=".csv" allowed="false" />
					 <add fileExtension=".cfg" allowed="false" />
					 <add fileExtension=".dev" allowed="false" />							 
                </fileExtensions>
            </requestFiltering>
		  <authorization>
			<!-- Deny all users access to the root of the website, since it
				 contains this web.config -->
				<remove users="*" roles="" verbs="" />
			<add accessType="Deny" users="*" />
		  </authorization>			
        </security>		
		
		<!-- REWRITE SECTION -->	
		<rewrite>

		  <!-- Custom Servername -->
		  <outboundRules rewriteBeforeCache="true">
		  </outboundRules>
		  
		  <rules>
                <rule name="CanonicalHostNameRule1" stopProcessing="true">
                    <match url="0.0.0.0" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="0.0.0.0" />
                    </conditions>
                    <action type="Redirect" url="enteryourdomain.com" />
                </rule>
		  
			<!-- ###################################
				 #                                 #
				 #          Other Stuff            #		 
				 #                                 #
				 ################################### -->
			
				<!-- Rewrite Robot.txt to Robot.php to add php code inside -->
                <rule name="Rewrite Tool - robots.txt to robots.php" stopProcessing="true">
                    <match url="^robots\.txt$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="robots.php" />
                </rule>
				
				<!-- Redirect HTTP to HTTPS -->	
				<rule name="HTTPS force" enabled="true" stopProcessing="true">
					<match url="(.*)" />
					<conditions>
					<add input="{HTTPS}" pattern="^OFF$" />
					</conditions>
					<action type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}" redirectType="Permanent" />
				</rule>					

				<!-- Redirect Server IP to Domain Name Canonical FIX -->
                <rule name="IP to Domain Redirect Hit" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="0.0.0.0" />
                    </conditions>
                    <action type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}" redirectType="Permanent" />
                </rule>		               	
				
			<!-- ###################################
				 #                                 #
				 #    Rewrite Home & Dashboard     #		 
				 #                                 #
				 ################################### -->

				<!-- Rewrite Home ( Landing Page ) -->
                <rule name="Rewrite Public - Home (Landing Page)" stopProcessing="true">
                    <match url="^home$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="System/Views/Welcome.php" />
                </rule>	
				
				<!-- Rewrite Admin Dashboard ( Dashboard Home ) -->
                <rule name="Rewrite Admin Dashboard - Home" stopProcessing="true">
                    <match url="^dashboard$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Index.php" />
                </rule>		
				
			<!-- ###################################
				 #                                 #
				 #  Rewrite Admin Login & Logout   #		 
				 #                                 #
				 ################################### -->
			
				<!-- Rewrite Admin Login -->
                <rule name="Rewrite Admin Dashboard - Login" stopProcessing="true">
                    <match url="^dashboard/signin$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Login.php" />
                </rule>
				
				<!-- Rewrite Admin Logout -->
                <rule name="Rewrite Admin Dashboard - Logout" stopProcessing="true">
                    <match url="^dashboard/signout$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Logout.php" />
                </rule>				
				
			<!-- ###################################
				 #                                 #
				 #   Rewrite Blog System Rules     #		 
				 #                                 #
				 ################################### -->	
				 
				<!-- Rewrite Articles -->
                <rule name="Rewrite Public - All Articles" stopProcessing="true">
                    <match url="^articles$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="System/Views/Articles.php" />
                </rule>

				<!-- Rewrite Article Post -->
                <rule name="Rewrite Public - View Article ID (WIP)" stopProcessing="true">
                    <match url="^article/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="System/Views/Viewarticle.php?id={R:1}" appendQueryString="true" />
                </rule>						
				
				<!-- Rewrite Admin Dashboard - All Articles -->
                <rule name="Rewrite Admin Dashboard - All Articles" stopProcessing="true">
                    <match url="^dashboard/articles$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Blog/Index.php" />
                </rule>						

				<!-- Rewrite Admin Dashboard - Create Article -->
                <rule name="Rewrite Admin Dashboard - Create Article" stopProcessing="true">
                    <match url="^dashboard/create-article$" />
                    <conditions logicalGrouping="MatchAll">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" ignoreCase="false" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="Adm/Blog/Create.php" />
                </rule>
				
				<!-- Rewrite Admin Dashboard - Edit Article -->
                <rule name="Rewrite Admin Dashboard - Edit Article" stopProcessing="true">
                    <match url="^dashboard/edit-article/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Blog/Edit.php?id={R:1}" appendQueryString="true" />
                </rule>					

				<!-- Rewrite Admin Dashboard - Delete Article -->
                <rule name="Rewrite Admin Dashboard - Delete Article" stopProcessing="true">
                    <match url="^dashboard/delete-article/(.*)$" ignoreCase="false" />
                    <action type="Rewrite" url="Adm/Blog/Delete.php?id={R:1}" appendQueryString="true" />
                </rule>						

			<!-- ###################################
				 #                                 #
				 #   Rewrite Page System Rules     #		 
				 #                                 #
				 ################################### -->	
				 
				<!-- Rewrite Pages -->
